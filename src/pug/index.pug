        // Footer
        footer.footer.text-center
            .container
                .row
                    // Footer Location
                    .col-lg-4.mb-5.mb-lg-0
                        h4.text-uppercase.mb-4 Location
                        p.lead.mb-0
                            | Milan
                            br
                            | Italy
                    // Footer Social Icons
                    .col-lg-4.mb-5.mb-lg-0
                        h4.text-uppercase.mb-4 Around the Web
                        a.btn.btn-outline-light.btn-social.mx-1(href='https://www.facebook.com/salvosq')
                            i.fab.fa-fw.fa-facebook-f
                        a.btn.btn-outline-light.btn-social.mx-1(href='https://www.instagram.com/ssprog')
                            i.fab.fa-fw.fa-instagram
                        a.btn.btn-outline-light.btn-social.mx-1(href='https://www.youtube.com/channel/UCwYHTLXMVzOFVXqlsjWrN6Q')
                            i.fab.fa-fw.fa-youtube
                        a.btn.btn-outline-light.btn-social.mx-1(href='https://www.tiktok.com/@ssprog')
                            i.fab.fa-fw.fa-tiktok
                    // Footer About Text
                    .col-lg-4
                        h4.text-uppercase.mb-4 SSProg personal page
                        p.lead.mb-0
                            | Developed with 
                            a(href='https://getbootstrap.com') Bootstrap
                            | .
        // Copyright Section
        div.copyright.py-4.text-center.text-white
            .container
                small Copyright Â© SSProg 2021
        // Scroll to Top Button (Only visible on small and extra-small screen sizes)
        .scroll-to-top.d-lg-none.position-fixed
            a.js-scroll-trigger.d-block.text-center.text-white.rounded(href='#page-top')
                i.fa.fa-chevron-up

